{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components": "./src/Components",
          "@pages": "./src/pages",
          "@styles": "./src/styles",
          "@hooks": "./src/hooks",
          "@constants": "./src/constants",
          "@modules": "./src/modules"
        }
      }
    ]
  ]
}
